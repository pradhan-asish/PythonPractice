   "source": [
    "import logging\n",
    "logging.basicConfig(level=logging.DEBUG, format = '%(asctime)s - %(levelname)s - %(message)s')\n",
    "\n",
    "def factorial(n):\n",
    "    fact = 1 ;\n",
    "    for i in range(1,n+1):\n",
    "        fact = fact * i\n",
    "    print('Factorial of the number:'+ str(fact))\n",
    "\n",
    "factorial(5)\n",
    "    \n",
    "    \n",
    "\n"
   ]
